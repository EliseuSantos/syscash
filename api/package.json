{
  "name": "syscash",
  "version": "0.0.1",
  "description": "syscash",
  "author": "s.eliseu.santos@gmail.com",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"__test__/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "lint": "eslint \"{src,apps,libs,__test__}/**/*.ts\" --fix",
    "test": "jest --passWithNoTests --config ./jest.config.json --runInBand",
    "test:watch": "jest --watch --config ./jest.config.json --runInBand",
    "test:coverage": "jest --coverage --passWithNoTests --no-cache --config ./jest.config.json --runInBand",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "precommit": "lint-staged",
    "seed": "node dist/src/infrastructure/seeders",
    "seed:refresh": "node dist/src/infrastructure/seeders --refresh"
  },
  "dependencies": {
    "@nestjs/axios": "^0.0.7",
    "@nestjs/common": "^8.4.0",
    "@nestjs/config": "^1.2.0",
    "@nestjs/core": "^8.4.0",
    "@nestjs/jwt": "^10.0.2",
    "@nestjs/mapped-types": "^1.0.1",
    "@nestjs/mongoose": "^9.0.2",
    "@nestjs/passport": "^9.0.3",
    "@nestjs/platform-express": "^8.4.0",
    "@nestjs/serve-static": "^2.2.2",
    "@nestjs/swagger": "^5.2.0",
    "@nestjs/terminus": "^8.0.4",
    "chalk": "^4.1.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "dotenv": "^16.0.0",
    "helmet": "^5.0.2",
    "lodash": "^4.17.21",
    "module-alias": "^2.2.2",
    "moment-timezone": "^0.5.34",
    "mongoose": "^6.2.5",
    "nestjs-seeder": "^0.3.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.5",
    "swagger-ui-express": "^4.3.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.2.2",
    "@nestjs/schematics": "^8.0.8",
    "@nestjs/testing": "^8.4.0",
    "@types/chance": "^1.1.3",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.1",
    "@types/lodash": "^4.14.179",
    "@types/node": "^17.0.21",
    "@types/passport-jwt": "^3.0.8",
    "@types/passport-local": "^1.0.35",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.14.0",
    "@typescript-eslint/parser": "^5.14.0",
    "chance": "^1.1.8",
    "cross-env": "^7.0.3",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import-helpers": "^1.2.1",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.5.1",
    "lint-staged": "^12.3.5",
    "prettier": "^2.5.1",
    "source-map-support": "^0.5.21",
    "supertest": "^6.2.2",
    "ts-jest": "^27.1.3",
    "ts-loader": "^9.2.7",
    "ts-node": "^10.7.0",
    "tsconfig-paths": "3.13.0",
    "typescript": "^4.6.2",
    "webpack": "^5.76.0",
    "webpack-node-externals": "^3.0.0"
